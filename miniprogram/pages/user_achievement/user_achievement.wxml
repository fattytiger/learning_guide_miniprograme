<view class="page_top">
  <view class='user_icon'>
    <image src='{{userInfo.avatarUrl}}' animation='{{user_icon}}'></image>
  </view>
  <view class='user_name'>
    <text>{{userInfo.nickName}}</text>
  </view>
  <view class='user_get'>
    <view class='font'>
      <text>总进度</text>
    </view>
    <view class='progress_warper'>
      <progress active='true' percent='{{progress}}' border-radius='20rpx' stroke-width='15rpx'></progress>
    </view>
    <view class='font_two'>
      <text>我的成就</text>
      <text></text>
    </view>
    <view bindtap='badage_wall' class='chievement_icon' animation="{{chieve}}">
      <image src='{{edage_image[range]}}'></image>
    </view>
  </view>
</view>
<view class='page_mid'>
  <view class='test'>
    <view class='mine_icon'>
      <image src='../../images/col.png' style='width:50rpx;height:50rpx;margin:15rpx;'></image>
    </view>
    <view class='detail' bindtap='into_collection'>
      <button>
      <view class='font'>我的收藏</view>
        <view class='icon'>
          <image src='../../images/section_icon_02.png'></image>
        </view></button>
    </view>
  </view>


  <view class=' test '>
    <view class='mine_icon'>
      <image src='../../images/test_icon.png ' style='width:50rpx;height:50rpx;margin:15rpx;'></image>
    </view>
    <view class='detail '>
      <button bindtap='into_test'><view class='font'>章节测试</view>
        <view class='icon'>
          <image src='../../images/section_icon_02.png'></image>
        </view></button>
    </view>
  </view>

  <view class=' test '>
    <view class='mine_icon'>
      <image src='../../images/connect.png ' style='width:50rpx;height:50rpx;margin:15rpx;'></image>
    </view>
    <view class='detail '>
        <button  open-type='feedback'><view class='font'>意见与反馈</view>
        <view class='icon'>
          <image src='../../images/section_icon_02.png'></image>
        </view></button>
    </view>
  </view>
  <view class='test '>
    <view class='mine_icon'>
      <image src='../../images/about_us.png ' style='width:50rpx;height:50rpx;margin:15rpx;'></image>
    </view>
    <view class='detail'>
      <button bindtap='about_us'>
        <view class='font'>关于我们</view>
        <view class='icon'>
          <image src='../../images/section_icon_02.png'></image>
        </view>
      </button>
    </view>
  </view>
</view>