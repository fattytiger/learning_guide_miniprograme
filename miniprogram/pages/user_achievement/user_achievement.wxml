<view class="page_top">
  <view class='user_icon'>
    <image src='{{userInfo.avatarUrl}}' animation='{{user_icon}}'></image>
  </view>
  <view class='user_name'>
    <text>{{userInfo.nickName}}</text>
  </view>
  <view class='user_get'>
    <view class='font'>
      <text>总进度</text>
    </view>
    <view class='progress_warper'>
      <progress active='true' percent='{{progress}}' border-radius='20rpx' stroke-width='15rpx'></progress>
    </view>
    <view class='font_two'>
      <text>我的成就</text>
      <text></text>
    </view>
    <view bindtap='badage_wall' class='chievement_icon' animation="{{chieve}}">
      <image src='{{edage_image[range]}}'></image>
    </view>
  </view>
</view>
<view class='page_mid'>
  <view class='test'>
    <view style='height:80rpx;width:80rpx;margin-top: 10rpx;margin-left: 20rpx;'>
      <image src='../../images/col.png' style='width:80rpx;height:80rpx;'></image>
    </view>
    <view class='detail' bindtap='into_collection'>
      <button  bindtap='into_collection'>我的收藏</button>
    </view>
  </view>


  <view class=' test '>
    <view style='height:80rpx;width:80rpx;margin-top: 10rpx;margin-left: 20rpx; '>
      <image src='../../images/test_icon.png ' style='width:80rpx;height:80rpx; '></image>
    </view>
    <view class='detail '>
      <button bindtap='into_test'>章节测试</button>
    </view>
  </view>

  <view class=' test '>
    <view style='height:80rpx;width:80rpx;margin-top: 10rpx;margin-left: 20rpx; '>
      <image src='../../images/connect.png ' style='width:80rpx;height:80rpx; '></image>
    </view>
    <view class='detail '>
        <button  open-type='feedback '>意见与反馈</button>
    </view>
  </view>
  <view class='test '>
    <view style='height:80rpx;width:80rpx;margin-top: 10rpx;margin-left: 20rpx; '>
      <image src='../../images/about_us.png ' style='width:80rpx;height:80rpx; '></image>
    </view>
    <view class='detail'>
      <button bindtap='about_us'>关于我们</button>
    </view>
  </view>
</view>