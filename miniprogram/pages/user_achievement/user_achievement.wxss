.page_top{height: 400rpx;width: 750rpx;position: relative;background: #a9c7c7}
.page_top>.user_icon{position: absolute;left: 50rpx;top: 50rpx;height: 150rpx;width: 150rpx}
.page_top>.user_icon image{height:150rpx;width: 150rpx;border-radius: 50%}
.page_top>.user_name{position: absolute;left: 50rpx;top: 250rpx;height: 50rpx;width: 150rpx;line-height: 50rpx;text-align: center;font-size: 30rpx;color: #fff;}
.page_top>.user_get{position: absolute;height: 300rpx;width: 450rpx;left: 250rpx;top: 50rpx;background: #4c8dae;z-index: 1;border-radius: 20rpx;}
.page_top>.user_get>.font{height: 50rpx;width:80rpx;font-size: 25rpx;position: absolute;left: 50rpx;top: 50rpx; line-height: 50rpx;color: #fff}
.page_top>.user_get>.progress_warper{height: 60rpx;width: 280rpx;position: absolute;top: 50rpx;left: 150rpx;}
.page_top>.user_get>.progress_warper progress{margin-top: 18rpx;}
.page_top>.user_get>.chievement_icon{position: absolute;height: 150rpx;width: 150rpx;left: 200rpx;top: 125rpx;}
.page_top>.user_get>.chievement_icon image{height: 150rpx;width: 150rpx}
.page_top>.user_get>.font_two{height: 100rpx;width:100rpx;font-size: 25rpx;position: absolute;left: 50rpx;top: 150rpx; line-height: 50rpx;color: #fff;}
.page_top>.user_get>.font_two text{display: inline-block;line-height: 40rpx;}
.page_top>.user_get>.font_two text:nth-of-type(2){color: #fff}
.page_mid{margin-top: 50rpx;}

.page_mid>.test{height: 100rpx;width: 750rpx;display: flex;flex-direction: row;margin: 20rpx 0;}
.page_mid>.test>.mine_icon{height:80rpx;width:80rpx;margin-top: 10rpx;margin-left:30rpx;}
.page_mid>.test>.detail{height:80rpx;width: 600rpx;font-size:25rpx;}
.page_mid>.test>.detail> button::after{border: none}
.page_mid>.test>.detail> button{height: 80rpx;width: 500rpx;line-height: 80rpx;font-size: 30rpx;border-radius: 0;text-align: left;background: #fff; border-bottom: 1rpx solid #eee;display: flex;flex-direction: row;padding-right: 0}
.page_mid>.test>.detail> button>.font{width: 420rpx;}
.page_mid>.test>.detail> button>.icon{height: 80rpx;width: 80rpx;margin-right: 0}
.page_mid>.test>.detail> button>.icon>image{height: 50rpx;width: 50rpx;margin: 15rpx}