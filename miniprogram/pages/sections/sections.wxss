.page_mid{width: 750rpx;margin-top: 10rpx;}
.page_mid>.section_warper{height: 150rpx;width: 100%;position: relative}
.page_mid>.section_warper::before{content: "";display: block;height: 0.01rpx}
.page_mid .section{height: 80rpx;width: 650rpx;margin-top: 30rpx;margin-left: 50rpx;position: relative;display: flex;flex-direction: row;border-radius: 10rpx;z-index: 3}
.page_mid .section .section_title{width: 500rpx;height: 60rpx;margin-top: 10rpx;margin-left: 20rpx;line-height: 60rpx;font-size: 25rpx;text-indent: 2em}
.page_mid>.section_hide>.hide_warper{width: 650rpx;height: 150rpx;position: relative;margin-left: 50rpx;margin-top: 10rpx;margin-bottom: 10rpx;}
.page_mid>.section_hide>.hide_warper::before{content: "";display: block;height: 0.01rpx;}
.page_mid>.section_hide .mini_section{width: 650rpx;height: 80rpx;font-size: 25rpx;margin-top: 34rpx;border-radius: 10rpx;display: flex;flex-direction: row}
.page_mid>.section_hide .mini_section>.mini_section_name{width: 550rpx;height: 80rpx;line-height: 80rpx;text-indent: 5em;}
::-webkit-scrollbar{width: 10rpx;height: 100rpx;color: green}
