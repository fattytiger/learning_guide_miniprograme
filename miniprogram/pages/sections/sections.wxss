.page_top{width: 750rpx;background: #60747d;height: 400rpx;position: relative}
.page_top>.banner{width: 650rpx;height: 450rpx;position: absolute;top: -400rpx;left: 50rpx;z-index: 1;background: #9f849b;border-radius: 20rpx;}
.page_top>.banner>.banner_title{width: 600rpx;height: 80rpx;position: absolute;top: 25rpx;left:-625rpx;z-index: 2;line-height: 80rpx;text-align: center;font-size: 30rpx;background: url(data:image/png;base64,) no-repeat}
.page_top>.banner>.target{width: 600rpx;height: 300rpx; position: absolute;top: 130rpx;left:625rpx;z-index: 2;}
.page_top>.banner>.target .target_title{position: absolute;left: 5rpx;top:10rpx;line-height: 50rpx;font-size: 30rpx;}
.page_top>.banner>.target .target_item{position: absolute;left: 5rpx;top: 70rpx;height: 200rpx; width: 590rpx;border-radius: 20rpx;overflow: hidden}
.page_top>.banner>.target .target_item .item{line-height: 40rpx;font-size: 25rpx;text-indent: 2em;}


.page_mid{background:#a3b2c7;width: 750rpx;padding-bottom: 20rpx;}
.page_mid::before{content: "";display: block;height:90rpx;}
.page_mid>.section{height: 80rpx;width: 650rpx;margin-top: 30rpx;margin-left: 50rpx;background: #d3e5f9;position: relative;display: flex;flex-direction: row;border-radius: 10rpx;z-index: 3}
.page_mid>.section>.section_title{width: 500rpx;height: 60rpx;margin-top: 10rpx;margin-left: 20rpx;line-height: 60rpx;font-size: 25rpx;}
.page_mid>.section>.section_option{width: 80rpx;height: 40rpx;margin-top: 20rpx; border: 1px solid #beacc2; border-radius: 10rpx; margin-left: 20rpx;line-height: 60rpx;background: url(data:image/png;base64,) no-repeat}
.page_mid>.section_hide>.mini_section{width: 650rpx;height: 80rpx;background: #fff;margin-left: 50rpx;line-height: 80rpx;font-size: 25rpx;text-indent: 2em;margin-top: 20rpx;border-radius: 10rpx;z-index: 2}

